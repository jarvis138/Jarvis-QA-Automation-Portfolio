from selenium import webdriver

# Set up the Selenium webdriver
driver = webdriver.Chrome()

# Open the website with the search functionality
driver.get("https://www.example.com")

# Find the search input field and enter the search query
search_input = driver.find_element_by_id("search-input")
search_query = "your search query"
search_input.send_keys(search_query)

# Submit the search form
search_button = driver.find_element_by_id("search-button")
search_button.click()

# Get the search results
search_results = driver.find_elements_by_class_name("search-result")

# Check if the search query is present in any of the search results
search_query_found = False
for result in search_results:
    if search_query in result.text:
        search_query_found = True
        break

# Assert whether the search query was found or not
assert search_query_found, "Search query not found in the search results."

# Close the browser
driver.quit()
